   "source": [
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)\n",
    "\n",
    "resultado = factorial(5)\n",
    "print(f\"El factorial de 5 es: {resultado}\")\n"
   ]
